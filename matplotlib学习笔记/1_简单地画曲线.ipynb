   "metadata": {},
   "outputs": [],
   "source": [
    "# 在[-1,1]内平均地生成50个点\n",
    "x = np.linspace(-1, 1, 50)\n",
    "# 定义一个方程\n",
    "y = 2 * x + 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
